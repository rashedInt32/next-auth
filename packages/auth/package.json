{
  "name": "@repo/auth",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./options": {
      "types": "./dist/options.d.ts",
      "default": "./src/options.ts"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "default": "./src/next.ts"
    },
    "./signup-route": {
      "types": "./dist/signup-route.d.ts",
      "default": "./src/signup-route.ts"
    },
    "./login": {
      "types": "./dist/login.d.ts",
      "default": "./src/login.ts"
    },
    "./error": {
      "types": "./dist/error.d.ts",
      "default": "./src/error.ts"
    },
    "./reset-password-route": {
      "types": "./dist/reset-password/reset-password-route.d.ts",
      "default": "./src/reset-password/reset-password-route.ts"
    },
    "./crypto-service": {
      "types": "./dist/service/jwt.d.ts",
      "default": "./src/service/jwt.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/db": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.31.0",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "6.13.0",
    "bcryptjs": "^3.0.2",
    "effect": "^3.17.6",
    "jose": "^6.0.12",
    "next": "^15.4.2",
    "next-auth": "5.0.0-beta.29",
    "resend": "^6.0.1"
  }
}
